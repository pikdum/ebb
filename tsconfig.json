{
	"compilerOptions": {
		"jsx": "react-jsx",
		"target": "ESNext",
		"module": "esnext",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"baseUrl": ".",
		"outDir": "dist",
		"moduleResolution": "bundler",
		"resolveJsonModule": true
	},
	"include": ["."]
}
